{
  "name": "pixnode",
  "version": "3.0.0",
  "description": "Pixiv API for node.js",
  "extends": "tsconfig.json",
  "author": "Hexona",
  "license": "MIT",
  "main": "./dist/index",
  "scripts": {
    "build": "tsc && tsconfig-replace-paths -p tsconfig.json -s ./src -o ./dist",
    "clean": "rimraf dist/*",
    "lint": "prettier \"./src/**/*.ts\" --write",
    "make": "npm run lint && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hexona69/pix-node.git"
  },
  "keywords": [
    "Pixiv",
    "node",
    "module",
    "pixiv"
  ],
  "bugs": {
    "url": "https://github.com/Hexona69/pix-node/issues"
  },
  "homepage": "https://github.com/Hexona69/pix-node#readme",
  "dependencies": {
    "axios": "^1.7.2",
    "bunyan": "^1.8.15",
    "node-schedule": "^2.1.1",
    "prettier": "^3.3.2",
    "readline-sync": "^1.4.10",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "tsconfig-replace-paths": "^0.0.14",
    "typescript": "^5.5.2",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/node": "^17.0.45",
    "@types/node-schedule": "^2.1.7",
    "@types/readline-sync": "^1.4.4"
  }
}
