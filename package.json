{
  "name": "pixnode",
  "version": "2.0.5",
  "description": "Pixiv API for node.js",
  "extends": "tsconfig.json",
  "author": "Hexona",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.2.0",
    "base64url": "^3.0.1",
    "prettier": "^3.3.2",
    "qs": "^6.11.0",
    "readline-sync": "^1.4.10",
    "rimraf": "^5.0.7",
    "tsconfig-replace-paths": "^0.0.14"
  },
  "main": "./dist/index",
  "scripts": {
    "build": "tsc && tsconfig-replace-paths -p tsconfig.json -s ./src -o ./dist",
    "clean": "rimraf dist/*",
    "lint": "prettier \"./src/**/*.ts\" --write",
    "make": "npm run lint && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hexona69/pix-node.git"
  },
  "keywords": [
    "Pixiv",
    "node",
    "module",
    "pixiv"
  ],
  "bugs": {
    "url": "https://github.com/Hexona69/pix-node/issues"
  },
  "homepage": "https://github.com/Hexona69/pix-node#readme",
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/qs": "^6.9.7",
    "@types/readline-sync": "^1.4.4",
    "typescript": "^4.6.3"
  }
}
